    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.11.21"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/miriam-samuels/s3-react"
  },
  "bugs": {
    "url": "https://github.com/miriam-samuels/s3-react/issues"
